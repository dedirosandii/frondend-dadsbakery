<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FoodMarketOnline" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <!--    Tanpa ada ActionBar  -->
    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
    </style>

    <!--    Overlay  -->
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">

    </style>
    <!--    Tab  -->
    <style name="Tab">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/abuputih</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">left</item>
    </style>

    <!--    RatingBar   -->
    <style name="RatingBar">
        <item name="colorControlNormal">@color/abu2</item>
        <item name="colorControlActivated">@color/kuningoren</item>
    </style>

    <!-- Style TextView -->
    <style name="TextView"/>

    <!--    Style untuk SplashScreen  -->
    <style name="TextView.PoppinsSplashScreen">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/hitamabu</item>
        <item name="android:textSize">32sp</item>
    </style>

    <!--    Style untuk Signin  -->
    <style name="TextView.PoppinsSignin">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/hitamabu</item>
        <item name="android:textSize">16sp</item>
    </style>

    <!--    Style untuk ToolBar Title  -->
    <style name="TextView.PoppinsToolBarTitle">
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:textColor">@color/hitamabu</item>
        <item name="android:textSize">22sp</item>
    </style>

    <!--    Style untuk Title Success  -->
    <style name="TextView.PoppinsTitleSuccess">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">@color/hitamabu</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="TextView.PoppinsProfile">
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">@color/hitamabu</item>
        <item name="android:textSize">18sp</item>
    </style>

    <!--    Style untuk ToolBar Sub  -->
    <style name="TextView.PoppinsToolBarSub">
        <item name="android:fontFamily">@font/poppins</item>
        <item name="android:textColor">@color/abuputih</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="EditText"/>
